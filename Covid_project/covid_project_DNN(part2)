   ],
   "source": [
    "num_epochs, best_loss, early_stop_count = config['n_epochs'], math.inf, 0\n",
    "train_info = {\"Loss\" : []}\n",
    "val_info = {\"Loss\" : []}\n",
    "\n",
    "for epoch in range(num_epochs):\n",
    "    train_loss = train(model, train_loader, criterion, optimizer, device, writer)\n",
    "    val_loss = validate(model, valid_loader, criterion, device, writer)\n",
    "\n",
    "    train_info[\"Loss\"].append(train_loss)\n",
    "\n",
    "    val_info[\"Loss\"].append(val_loss)\n",
    "    \n",
    "    print(f'[{epoch+1:03d}/{num_epochs:03d}] Train_Loss: {train_loss:.5f} | Valid_Loss: {val_loss:.5f}')\n",
    "\n",
    "    if val_loss < best_loss:\n",
    "        best_loss = val_loss\n",
    "        torch.save(model.state_dict(), config['save_path'])\n",
    "        print(f'Saving model with acc {best_loss:.5f}')\n",
    "        \n",
    "        early_stop_count = 0\n",
    "    else: \n",
    "        early_stop_count += 1\n",
    "        \n",
    "    if early_stop_count >= config['early_stop']:\n",
    "        print('\\nModel is not improving, so we halt the training session.')\n",
    "        break"
   ]
  },
